<%- model_class = CustomTailoringShopped -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:preset_name) %></th>
      <th><%= model_class.human_attribute_name(:front_neck_style_value) %></th>
      <th><%= model_class.human_attribute_name(:back_neck_style_value) %></th>
      <th><%= model_class.human_attribute_name(:sleeve_style_value) %></th>
      <th><%= model_class.human_attribute_name(:front_neck_style_name) %></th>
      <th><%= model_class.human_attribute_name(:back_neck_style_name) %></th>
      <th><%= model_class.human_attribute_name(:sleeve_style_name) %></th>
      <th><%= model_class.human_attribute_name(:fitting) %></th>
      <th><%= model_class.human_attribute_name(:blouse_opening) %></th>
      <th><%= model_class.human_attribute_name(:fall_and_edging) %></th>
      <th><%= model_class.human_attribute_name(:lining) %></th>
      <th><%= model_class.human_attribute_name(:petticoat_inskirt) %></th>
      <th><%= model_class.human_attribute_name(:around_bust) %></th>
      <th><%= model_class.human_attribute_name(:shoulder) %></th>
      <th><%= model_class.human_attribute_name(:natural_waist) %></th>
      <th><%= model_class.human_attribute_name(:around_arm_hole) %></th>
      <th><%= model_class.human_attribute_name(:around_arm) %></th>
      <th><%= model_class.human_attribute_name(:your_height) %></th>
      <th><%= model_class.human_attribute_name(:front_neck_depth) %></th>
      <th><%= model_class.human_attribute_name(:back_neck_depth) %></th>
      <th><%= model_class.human_attribute_name(:sleeve_length) %></th>
      <th><%= model_class.human_attribute_name(:blouse_length) %></th>
      <th><%= model_class.human_attribute_name(:product_id) %></th>
      <th><%= model_class.human_attribute_name(:product_name) %></th>
      <th><%= model_class.human_attribute_name(:customer_id) %></th>
      <th><%= model_class.human_attribute_name(:order_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @custom_tailoring_shoppeds.each do |custom_tailoring_shopped| %>
      <tr>
        <td><%= link_to custom_tailoring_shopped.id, custom_tailoring_shopped_path(custom_tailoring_shopped) %></td>
        <td><%= custom_tailoring_shopped.preset_name %></td>
        <td><%= custom_tailoring_shopped.front_neck_style_value %></td>
        <td><%= custom_tailoring_shopped.back_neck_style_value %></td>
        <td><%= custom_tailoring_shopped.sleeve_style_value %></td>
        <td><%= custom_tailoring_shopped.front_neck_style_name %></td>
        <td><%= custom_tailoring_shopped.back_neck_style_name %></td>
        <td><%= custom_tailoring_shopped.sleeve_style_name %></td>
        <td><%= custom_tailoring_shopped.fitting %></td>
        <td><%= custom_tailoring_shopped.blouse_opening %></td>
        <td><%= custom_tailoring_shopped.fall_and_edging %></td>
        <td><%= custom_tailoring_shopped.lining %></td>
        <td><%= custom_tailoring_shopped.petticoat_inskirt %></td>
        <td><%= custom_tailoring_shopped.around_bust %></td>
        <td><%= custom_tailoring_shopped.shoulder %></td>
        <td><%= custom_tailoring_shopped.natural_waist %></td>
        <td><%= custom_tailoring_shopped.around_arm_hole %></td>
        <td><%= custom_tailoring_shopped.around_arm %></td>
        <td><%= custom_tailoring_shopped.your_height %></td>
        <td><%= custom_tailoring_shopped.front_neck_depth %></td>
        <td><%= custom_tailoring_shopped.back_neck_depth %></td>
        <td><%= custom_tailoring_shopped.sleeve_length %></td>
        <td><%= custom_tailoring_shopped.blouse_length %></td>
        <td><%= custom_tailoring_shopped.product_id %></td>
        <td><%= custom_tailoring_shopped.product_name %></td>
        <td><%= custom_tailoring_shopped.customer_id %></td>
        <td><%= custom_tailoring_shopped.order_id %></td>
        <td><%=l custom_tailoring_shopped.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_custom_tailoring_shopped_path(custom_tailoring_shopped), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      custom_tailoring_shopped_path(custom_tailoring_shopped),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_custom_tailoring_shopped_path,
            :class => 'btn btn-primary' %>
