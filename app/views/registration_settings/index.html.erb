<p id="notice"><%= notice %></p>

<h1>Listing Registration Settings</h1>

<table>
  <thead>
    <tr>
      <th>Value of coupon</th>
      <th>Validity of coupon</th>
      <th>No of coupons</th>
      <th>No of times coupon use</th>
      <th>Price range for coupon to valid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_settings.each do |registration_setting| %>
      <tr>
        <td><%= registration_setting.Value_of_coupon %></td>
        <td><%= registration_setting.Validity_of_coupon %></td>
        <td><%= registration_setting.No_of_Coupons %></td>
        <td><%= registration_setting.No_of_times_coupon_use %></td>
        <td><%= registration_setting.price_range_for_coupon_to_valid %></td>
        <td><%= link_to 'Show', registration_setting %></td>
        <td><%= link_to 'Edit', edit_registration_setting_path(registration_setting) %></td>
        <td><%= link_to 'Destroy', registration_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration setting', new_registration_setting_path %>
