<p id="notice"><%= notice %></p>

<h1>Listing Registration Coupons</h1>

<table>
  <thead>
    <tr>
      <th>Coupon value</th>
      <th>Price range</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registration_coupons.each do |registration_coupon| %>
      <tr>
        <td><%= registration_coupon.coupon_value %></td>
        <td><%= registration_coupon.price_range %></td>
        <td><%= link_to 'Show', registration_coupon %></td>
        <td><%= link_to 'Edit', edit_registration_coupon_path(registration_coupon) %></td>
        <td><%= link_to 'Destroy', registration_coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration coupon', new_registration_coupon_path %>
